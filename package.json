{
  "name": "vue-waterall-virtuallist",
  "version": "0.0.3",
  "description": "waterfall virtuallist plugin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dajiaman/vue-waterfall-virtuallist.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "unpkg": "dist/index.min.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "waterfall",
    "virtuallist",
    "vue-waterfall-virtuallist",
    "vue-waterall"
  ],
  "scripts": {
    "build": "rimraf ./dist && npm run rollup:build",
    "rollup:build": "node scripts/rollup.config.js",
    "start": "esno src/index.ts",
    "dev": "unbuild --stub",
    "demo:dev": "vite",
    "demo:build": "vue-tsc && vite build",
    "preview": "vite preview",
    "release": "release-it",
    "commit": "git-cz"
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "esno": "^0.16.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "release-it": "14.0",
    "rimraf": "^4.4.0",
    "rollup": "^3.19.1",
    "rollup-plugin-uglify": "^6.0.4",
    "typescript": "^4.9.3",
    "unbuild": "^1.1.2",
    "uniqolor": "^1.1.0",
    "uuid": "^9.0.0",
    "vite": "^4.1.0",
    "vue": "^3.2.45",
    "vue-tsc": "^1.0.24"
  },
  "git": {
    "commitMessage": "chore: release v${version}",
    "commit": true,
    "tag": true,
    "push": true
  },
  "github": {
    "release": true
  },
  "npm": {
    "publish": false
  }
}